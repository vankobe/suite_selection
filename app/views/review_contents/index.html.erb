<h1>Listing review_contents</h1>

<table>
  <tr>
    <th>Review</th>
    <th>Comment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @review_contents.each do |review_content| %>
  <tr>
    <td><%= review_content.review_id %></td>
    <td><%= review_content.comment %></td>
    <td><%= link_to 'Show', review_content %></td>
    <td><%= link_to 'Edit', edit_review_content_path(review_content) %></td>
    <td><%= link_to 'Destroy', review_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Review content', new_review_content_path %>
